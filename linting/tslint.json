{
  "extends": "tslint-microsoft-contrib",
  "rules": {
    "ter-indent": [
      true,
      2
    ],
    "ter-max-len": [
      true,
      140
    ],
    "one-variable-per-declaration": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ]
  },
  "rulesDirectory": [
    "../../../tslint-microsoft-contrib",
    "../../../tslint-eslint-rules/dist/rules"
  ]
}
